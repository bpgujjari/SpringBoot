<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>
<bean class="com.yakub.AddressBean" id="tempAddr">
<property name="hno" value="101"/>
<property name="street" value="ChimtuNagar"/>
<property name="city" value="Jayagiri"/>
</bean>
<bean class="com.yakub.AddressBean" id="perminentAddr">
<property name="hno" value="102"/>
<property name="street" value="Jayagiri"/>
<property name="city" value="Hyderabad"/>
</bean>

<bean class="com.yakub.EmployeeBean" id="id2">
<property name="eid" value="111"/>
<property name="ename" value="ChimtuModel"/>
</bean>



</beans>