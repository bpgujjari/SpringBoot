package com.ipl.service;

import org.springframework.beans.factory.annotation.Autowired;

import com.ipl.entity.IPLPointsTable;

public class IplServImp  implements IplServ{

	@Autowired
	@Override
	public void saveTeam() {
	IPLPointsTable p=new IPLPointsTable();
	p.setTeam("MI");
	p.setLoss(0);
	p.setNoResult(0);
	p.setNrr(0);
	p.setPlayed(0);
	p.setPosition(0);
	p.setWin(0);
	p.setPoints(0);
	}

}
